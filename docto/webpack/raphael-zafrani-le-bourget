// Other telesecretaryships use https://pro.doctolib.fr/admin/front-desk which also loads the calendar
// in an iframe from there.
//
// We must distinguish between these two cases to avoid monkey patching document.cookie when we remain
// in the same domain (pro.doctolib.fr).
const loadedInIframeOtherThanDoctolib = (function inIframe () {
  try {
      return window.self !== window.top && window.self.location.origin !== window.top.location.origin
  } catch (e) {
      return true;
  }
})();

// Didomi sets their 2 cookies (didomi_token and euconsent-v2) without the SameSite flag.
//
// By default, in March 2024, Chrome defaults to SameSite=Lax when the SameSite flag is missing,
// which prevents us from setting these 2 cookies at all, when Doctolib is loaded in an iframe.
// Firefox will soon follow and adopt the same behavior. But right now, it defaults to SameSite=None, which works.
//
// To persist these 2 cookies and avoid asking the users for consent every time the open Doctolib in an iframe,
// we must set the SameSite=None flag.
//
// Also, end of 2024, Chrome plans on blocking third party cookies that do not set the Partitioned flag.
// Therefore we are adding it.

if (loadedInIframeOtherThanDoctolib) {
  const cookieDesc = Object.getOwnPropertyDescriptor(
    Document.prototype,
    "cookie"
  );
  if (cookieDesc && cookieDesc.configurable) {
    Object.defineProperty(document, "cookie", {
      get: function () {
        return cookieDesc.get.call(document);
      },
      set: function (val) {
        let newValue = val;
        if (
          newValue.startsWith("didomi_token=") ||
          newValue.startsWith("euconsent-v2=")
        ) {
          if (newValue.toLowerCase().includes("samesite=")) {
            newValue = newValue.replace(/SameSite=\w+/i, "SameSite=None");
          } else {
            newValue += "; SameSite=None";
          }
          if (!newValue.toLowerCase().includes("partitioned")) {
            newValue += "; Partitioned";
          }
        }

        cookieDesc.set.call(document, newValue);
      },
    });
  }
}

window.gdprAppliesGlobally = true;
window.didomiEventListeners = window.didomiEventListeners || [];
window.didomiEventListeners.push({
  event: 'notice.shown',
  listener: function () {
    if (window.onDidomiNoticeShown) {
      window.onDidomiNoticeShown();
    }
    else {
      window.didomiNoticeShown = true;
    }
  }
});
